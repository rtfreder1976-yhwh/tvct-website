// FAQ Schema data for AEO optimization
const faqSchema = {
  "@context": "https://schema.org",
  "@type": "FAQPage",
  "mainEntity": [
    {
      "@type": "Question",
      "name": "When should I schedule spring cleaning in Nashville, TN?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "The best time for spring cleaning in Nashville is mid-March to early April. Wait until the worst pollen has passed (usually late March), but complete cleaning before summer humidity arrives. Book 3-4 weeks ahead‚ÄîNashville cleaning services fill up fast during spring."
      }
    },
    {
      "@type": "Question",
      "name": "How much does spring cleaning cost in Nashville?",
