---
import BaseLayout from '../../layouts/BaseLayout.astro';
import Navigation from '../../components/Navigation.astro';
import Footer from '../../components/Footer.astro';
import SchemaMarkup from '../../components/SchemaMarkup.astro';
import TrustBadges from '../../components/TrustBadges.astro';
import Icon from '../../components/Icon.astro';

const title = "Move In/Out Cleaning ";
const description = "Professional move in/out cleaning in AL & TN. Empty home deep cleaning for landlords & renters. Get your deposit back. Same-day available.";

// Service schema
const serviceSchema = {
  "@context": "https://schema.org",
  "@type": "Service",
  "name": "Move In/Out Cleaning Service",
  "description": "Professional move in/out cleaning service for empty homes. Thorough cleaning of all rooms, appliances, cabinets, and fixtures to ensure properties are move-in ready or deposit-back clean.",
  "provider": {
    "@type": "LocalBusiness",
    "name": "The Valley Clean Team",
    "telephone": "256-826-1100",
    "email": "info@thevalleycleanteam.com",
    "address": {
      "@type": "PostalAddress",
      "addressLocality": "Huntsville",
      "addressRegion": "AL",
      "addressCountry": "US"
    }
  },
  "areaServed": [
    { "@type": "State", "name": "Alabama" },
    { "@type": "State", "name": "Tennessee" }
  ],
  "serviceType": "Move In/Out Cleaning",
  "offers": {
    "@type": "Offer",
    "priceSpecification": {
      "@type": "PriceSpecification",
      "price": "199",
      "priceCurrency": "USD",
      "minPrice": "199"
    },
    "availability": "https://schema.org/InStock"
  }
};

// FAQ Schema - condensed for optimal word count
const faqs = [
  {
    question: "What's included in move in/out cleaning?",
    answer: "All rooms top to bottom: floors, baseboards, windows, light fixtures, bathroom fixtures, kitchen appliances (inside and out), cabinets (inside and out), and closets."
  },
  {
    question: "How much does move in/out cleaning cost?",
    answer: "Starts at $351 for homes up to 1,500 sq ft. Medium homes (1,500-2,500 sq ft) start at $456, and larger homes are priced accordingly. Free quotes available."
  },
  {
    question: "Will this help me get my deposit back?",
    answer: "Yes! We clean to landlord standards. Many clients receive full deposits back. We provide before/after photos upon request."
  },
  {
    question: "How long does it take?",
    answer: "Typically 4-8 hours. A 3-bedroom, 2-bath home usually takes 5-6 hours."
  },
  {
    question: "Do you clean inside cabinets and appliances?",
    answer: "Yes! Inside all cabinets, drawers, closets, refrigerator, and oven are included standard."
  },
  {
    question: "Can you clean before my walkthrough?",
    answer: "Yes! Book 2-3 days ahead for best scheduling. Same-day service available when possible."
  },
  {
    question: "Do you remove scuff marks from walls?",
    answer: "Yes, we spot clean walls to remove scuff marks and minor stains."
  },
  {
    question: "Can you work around my moving schedule?",
    answer: "Yes! We can clean before or after movers, or in stages. We're flexible with your timeline."
  }
];

const faqSchema = {
  "@context": "https://schema.org",
  "@type": "FAQPage",
  "mainEntity": faqs.map(faq => ({
    "@type": "Question",
    "name": faq.question,
    "acceptedAnswer": {
      "@type": "Answer",
      "text": faq.answer
    }
  }))
};

// Move In/Out specific checklist - tasks marked as moveInOut: true in the original checklist
const checklistData = [
  {
    section: "All Rooms",
    icon: "home",
    tasks: [
      "Remove cobwebs from all corners & ceilings",
      "Sweep/vacuum all floors (carpet + hard surfaces)",
      "Mop all hard surface floors",
      "Dust all shelves & surfaces",
      "Blind cleaning",
      "Dust windows & window sills",
      "Wipe & clean all mirrors",
      "Wipe & clean all light switches",
      "Clean all glass surfaces",
      "Dust all baseboards",
      "Dust reachable ceiling fans & vents",
      "Dust doors & door frames",
      "Clean light fixtures",
      "Spot clean walls (scuff marks, fingerprints)"
    ]
  },
  {
    section: "Kitchen",
    icon: "kitchen-set",
    tasks: [
      "Clean inside & outside of refrigerator",
      "Clean inside & outside of oven",
      "Inside/outside of microwave oven",
      "Clean backsplash",
      "Outside of stove hood",
      "Clean stovetop & drip pans",
      "All countertops",
      "Clean sink & polish chrome fixtures",
      "Inside & outside of all cabinets & drawers",
      "Pantry shelves, floor & door",
      "Top of refrigerator",
      "Clean dishwasher interior"
    ]
  },
  {
    section: "Bathrooms",
    icon: "bath",
    tasks: [
      "Deep clean shower & grout",
      "Clean tub thoroughly",
      "Polish all faucets & fixtures",
      "Clean & sanitize sinks",
      "Clean & sanitize toilet and toilet area",
      "Clean counters & vanity",
      "Dust all ledges",
      "Inside & outside of cabinets and drawers",
      "Clean medicine cabinet",
      "Dust doors & door frames",
      "Clean exhaust fan",
      "Clean mirrors streak-free"
    ]
  },
  {
    section: "Bedrooms & Closets",
    icon: "bed",
    tasks: [
      "Vacuum all floors & carpets",
      "Inside & outside of closets",
      "Clean closet shelves & rods",
      "Exterior of cabinets and drawers",
      "Dust all ledges & window sills",
      "Dust doors & door frames",
      "Clean light fixtures",
      "Spot clean walls"
    ]
  },
  {
    section: "Laundry Room",
    icon: "shirt",
    tasks: [
      "Dust and wipe outside of washer & dryer",
      "Clean behind washer & dryer (if accessible)",
      "Clean sinks",
      "All countertops & surfaces",
      "Dust ledges",
      "Inside & outside of cabinets",
      "Dust doors & door frames"
    ]
  },
  {
    section: "Living Areas",
    icon: "couch",
    tasks: [
      "Dust all ledges & surfaces",
      "Inside & outside of cabinets & built-ins",
      "Clean fireplace exterior (if applicable)",
      "Dust doors & door frames",
      "Clean all windows & tracks",
      "Vacuum/mop all floors",
      "Spot clean walls"
    ]
  }
];

// Extra services for move in/out
const extraServices = [
  { name: "Garage sweep out", icon: "warehouse" },
  { name: "Paint touch-ups", icon: "paint-roller" }
];

const commonProblems = [
  {
    icon: "money-bill-wave",
    problem: "Deposit at Risk",
    solution: "We clean to landlord standards for full deposit return, including inside cabinets and appliances."
  },
  {
    icon: "clock",
    problem: "Tight Timeline",
    solution: "We work around your schedule - before movers, after they leave, or same-day."
  },
  {
    icon: "broom",
    problem: "Previous Tenant's Mess",
    solution: "Moving into a dirty home? We'll make it spotless before you unpack."
  },
  {
    icon: "box-open",
    problem: "Hidden Dirt Behind Furniture",
    solution: "Empty homes reveal hidden dirt. We clean behind appliances and inside every cabinet."
  }
];

const processSteps = [
  {
    number: 1,
    title: "Schedule & Quote",
    description: "Tell us your move date and home size. We'll provide a clear quote - even same-day if needed.",
    icon: "calendar-check"
  },
  {
    number: 2,
    title: "Top-to-Bottom Clean",
    description: "Ceiling to floor, room by room. Dust and debris captured, not spread.",
    icon: "arrow-down-long"
  },
  {
    number: 3,
    title: "Kitchen Deep Clean",
    description: "Inside oven, refrigerator, dishwasher, all cabinets. Stovetop degreased.",
    icon: "kitchen-set"
  },
  {
    number: 4,
    title: "Bathroom Sanitization",
    description: "All fixtures scrubbed and sanitized, including inside cabinets.",
    icon: "bath"
  },
  {
    number: 5,
    title: "Detail Work",
    description: "Baseboards, window sills, closets - areas that matter for inspections.",
    icon: "magnifying-glass"
  },
  {
    number: 6,
    title: "Final Quality Check",
    description: "Complete inspection. Photos available for landlord records.",
    icon: "check-double"
  }
];

const serviceAreas = [
  { name: "Huntsville", link: "/locations/huntsville/move-out-cleaning" },
  { name: "Madison", link: "/locations/madison/move-out-cleaning" },
  { name: "Athens", link: "/locations/athens/move-out-cleaning" },
  { name: "Florence", link: "/locations/florence/move-out-cleaning" },
  { name: "Decatur", link: "/locations/decatur/move-out-cleaning" },
  { name: "Muscle Shoals", link: "/locations/muscle-shoals/move-out-cleaning" },
  { name: "Birmingham", link: "/locations/birmingham/move-out-cleaning" },
  { name: "Mountain Brook", link: "/locations/mountain-brook/move-out-cleaning" },
  { name: "Nashville", link: "/locations/nashville/move-out-cleaning" },
  { name: "West Nashville", link: "/locations/west-nashville/move-out-cleaning" }
];
---

<BaseLayout title={title} description={description}>
  <SchemaMarkup type="organization" />
  <script type="application/ld+json" set:html={JSON.stringify(serviceSchema)} />
  <script type="application/ld+json" set:html={JSON.stringify(faqSchema)} />
  <Navigation />

  <!-- Hero Section -->
  <section class="pt-32 pb-16 px-6 bg-gradient-to-br from-white to-gray-50">
    <div class="container mx-auto">
      <div class="grid lg:grid-cols-2 gap-12 items-center">
        <div>
          <div class="inline-block bg-gradient-to-r from-[#FFA985]/10 to-[#FFC67D]/10 px-4 py-2 rounded-full mb-4">
            <span class="text-[#FFA985] font-semibold">
              <Icon name="truck-moving" class="w-5 h-5 mr-2" />Moving? We've Got You Covered
            </span>
          </div>
          <h1 class="text-4xl md:text-5xl lg:text-6xl font-bold mb-6 leading-tight text-[#333333]">
            Move In/Out <span class="gold-gradient">Cleaning Services</span>
          </h1>
          <p class="text-xl text-gray-700 mb-6">
            Whether you're moving out and need your deposit back, or moving in and want a fresh start - our thorough move in/out cleaning ensures every surface is spotless. Inside cabinets, appliances, and all the areas hidden by furniture.
          </p>

          <!-- Key Benefits -->
          <div class="grid grid-cols-2 gap-4 mb-8">
            <div class="flex items-center text-gray-700">
              <Icon name="check-circle" class="w-5 h-5 text-[#FFA985] mr-2" />
              <span>Get Your Deposit Back</span>
            </div>
            <div class="flex items-center text-gray-700">
              <Icon name="check-circle" class="w-5 h-5 text-[#FFA985] mr-2" />
              <span>Inside Cabinets & Appliances</span>
            </div>
            <div class="flex items-center text-gray-700">
              <Icon name="check-circle" class="w-5 h-5 text-[#FFA985] mr-2" />
              <span>Same-Day Available</span>
            </div>
            <div class="flex items-center text-gray-700">
              <Icon name="check-circle" class="w-5 h-5 text-[#FFA985] mr-2" />
              <span>Before/After Photos</span>
            </div>
          </div>

          <div class="flex flex-col sm:flex-row gap-4">
            <a href="tel:2568261100" class="btn-primary px-8 py-4 rounded-full text-lg font-semibold text-center shadow-lg">
              <Icon name="phone" class="w-5 h-5 mr-2" />Call 256-826-1100
            </a>
            <a href="/contact" class="btn-secondary px-8 py-4 rounded-full text-lg font-semibold text-center">
              Get Free Quote
            </a>
          </div>
        </div>

        <!-- Pricing Card -->
        <div class="bg-white rounded-2xl p-8 shadow-2xl border-2 border-[#FFA985]/20">
          <div class="text-center mb-6">
            <h3 class="text-2xl font-bold text-[#333333] mb-2">Move In/Out Pricing</h3>
            <p class="text-gray-600">Includes inside cabinets & appliances</p>
          </div>

          <div class="space-y-4 mb-6">
            <div class="flex justify-between items-center p-4 bg-gray-50 rounded-xl">
              <div>
                <h4 class="font-bold text-[#333333]">Small Home</h4>
                <p class="text-sm text-gray-600">Up to 1,500 sq ft</p>
              </div>
              <div class="text-right">
                <span class="text-2xl font-bold text-[#FFA985]">From $351</span>
              </div>
            </div>

            <div class="flex justify-between items-center p-4 bg-gradient-to-r from-[#FFA985]/10 to-[#FFC67D]/10 rounded-xl border-2 border-[#FFA985]">
              <div>
                <h4 class="font-bold text-[#333333]">Medium Home</h4>
                <p class="text-sm text-gray-600">1,500 - 2,500 sq ft</p>
              </div>
              <div class="text-right">
                <span class="text-2xl font-bold text-[#FFA985]">From $456</span>
                <p class="text-xs text-gray-600">Most Common</p>
              </div>
            </div>

            <div class="flex justify-between items-center p-4 bg-gray-50 rounded-xl">
              <div>
                <h4 class="font-bold text-[#333333]">Large Home</h4>
                <p class="text-sm text-gray-600">2,500 - 4,000 sq ft</p>
              </div>
              <div class="text-right">
                <span class="text-2xl font-bold text-[#FFA985]">From $636</span>
              </div>
            </div>

            <div class="flex justify-between items-center p-4 bg-gray-50 rounded-xl">
              <div>
                <h4 class="font-bold text-[#333333]">XL Home</h4>
                <p class="text-sm text-gray-600">4,000+ sq ft</p>
              </div>
              <div class="text-right">
                <span class="text-2xl font-bold text-[#FFA985]">From $856</span>
              </div>
            </div>
          </div>

          <div class="bg-blue-50 border border-blue-200 rounded-xl p-4 mb-6">
            <p class="text-blue-800 text-sm text-center">
              <Icon name="info-circle" class="w-5 h-5 mr-2" />
              <strong>Note:</strong> Move in/out includes inside oven, fridge & cabinets (usually add-ons)
            </p>
          </div>

          <a href="/contact" class="btn-primary w-full py-4 rounded-lg text-lg font-semibold text-center block shadow-lg">
            Get Your Free Quote
          </a>
          <p class="text-center text-xs text-gray-500 mt-3">
            <Icon name="clock" class="w-5 h-5 mr-1" /> Response within 2 hours
          </p>
        </div>
      </div>
    </div>
  </section>

  <!-- Move Out vs Move In Comparison -->
  <section class="py-12 px-6 bg-gray-50">
    <div class="container mx-auto">
      <div class="grid md:grid-cols-2 gap-8 max-w-4xl mx-auto">
        <div class="bg-white rounded-2xl p-8 shadow-lg border-2 border-[#FFA985]/30">
          <div class="flex items-center mb-4">
            <div class="w-12 h-12 rounded-full bg-gradient-to-br from-[#FFA985] to-[#FFC67D] flex items-center justify-center mr-4">
              <Icon name="door-open" class="w-5 h-5 text-white" />
            </div>
            <h3 class="text-2xl font-bold text-[#333333]">Move Out</h3>
          </div>
          <p class="text-gray-600 mb-4">Leaving a rental? Selling your home?</p>
          <ul class="space-y-2">
            <li class="flex items-start text-gray-700">
              <Icon name="check" class="w-5 h-5 text-[#FFA985] mr-2 mt-1" />
              <span>Get your full deposit back</span>
            </li>
            <li class="flex items-start text-gray-700">
              <Icon name="check" class="w-5 h-5 text-[#FFA985] mr-2 mt-1" />
              <span>Pass landlord inspection</span>
            </li>
            <li class="flex items-start text-gray-700">
              <Icon name="check" class="w-5 h-5 text-[#FFA985] mr-2 mt-1" />
              <span>Leave on good terms</span>
            </li>
            <li class="flex items-start text-gray-700">
              <Icon name="check" class="w-5 h-5 text-[#FFA985] mr-2 mt-1" />
              <span>Photos for documentation</span>
            </li>
          </ul>
        </div>

        <div class="bg-white rounded-2xl p-8 shadow-lg border-2 border-green-200">
          <div class="flex items-center mb-4">
            <div class="w-12 h-12 rounded-full bg-gradient-to-br from-green-400 to-green-600 flex items-center justify-center mr-4">
              <Icon name="home" class="w-5 h-5 text-white" />
            </div>
            <h3 class="text-2xl font-bold text-[#333333]">Move In</h3>
          </div>
          <p class="text-gray-600 mb-4">New home? New apartment?</p>
          <ul class="space-y-2">
            <li class="flex items-start text-gray-700">
              <Icon name="check" class="w-5 h-5 text-green-500 mr-2 mt-1" />
              <span>Start fresh in a clean home</span>
            </li>
            <li class="flex items-start text-gray-700">
              <Icon name="check" class="w-5 h-5 text-green-500 mr-2 mt-1" />
              <span>Sanitize before unpacking</span>
            </li>
            <li class="flex items-start text-gray-700">
              <Icon name="check" class="w-5 h-5 text-green-500 mr-2 mt-1" />
              <span>Clean what previous owners left</span>
            </li>
            <li class="flex items-start text-gray-700">
              <Icon name="check" class="w-5 h-5 text-green-500 mr-2 mt-1" />
              <span>Peace of mind from day one</span>
            </li>
          </ul>
        </div>
      </div>
    </div>
  </section>

  <!-- Trust Badges -->
  <section class="py-8 px-6">
    <div class="container mx-auto">
      <TrustBadges variant="full" showGuarantee={true} />
    </div>
  </section>

  <!-- What's Included Section -->
  <section class="py-20 px-6 bg-gray-50 content-visibility-auto">
    <div class="container mx-auto">
      <div class="text-center mb-16">
        <h2 class="text-4xl md:text-5xl font-bold mb-4 text-[#333333]">
          What's <span class="gold-gradient">Included</span>
        </h2>
        <div class="section-divider"></div>
        <p class="text-xl text-gray-600 max-w-3xl mx-auto">
          Our move in/out cleaning is more comprehensive than standard cleaning. We clean inside cabinets, appliances, and all the areas that are normally inaccessible.
        </p>
      </div>

      <!-- Room-by-Room Checklist Grid -->
      <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-6">
        {checklistData.map(room => (
          <div class="bg-white rounded-2xl p-6 shadow-lg border border-gray-200 hover:shadow-xl transition-shadow">
            <div class="w-14 h-14 rounded-full bg-gradient-to-br from-[#FFA985] to-[#FFC67D] flex items-center justify-center mb-4">
              <i class={`fas ${room.icon} text-white text-xl`}></i>
            </div>
            <h3 class="text-xl font-bold mb-4 text-[#333333]">{room.section}</h3>
            <ul class="space-y-2">
              {room.tasks.map(task => (
                <li class="flex items-start text-gray-600 text-sm">
                  <Icon name="check" class="w-5 h-5 text-[#FFA985] mr-2 mt-1 flex-shrink-0" />
                  <span>{task}</span>
                </li>
              ))}
            </ul>
          </div>
        ))}
      </div>

      <!-- Task Count Summary -->
      <div class="mt-8 text-center">
        <p class="text-gray-600">
          <span class="font-bold text-[#FFA985] text-2xl">{checklistData.reduce((acc, room) => acc + room.tasks.length, 0)}+</span>
          <span class="text-lg"> cleaning tasks including </span>
          <span class="font-bold text-[#FFA985]">inside cabinets & appliances</span>
        </p>
      </div>

      <!-- Extra Services -->
      <div class="mt-12 bg-white rounded-2xl p-8 shadow-lg border border-gray-200">
        <h3 class="text-2xl font-bold text-[#333333] mb-2 text-center">Available Add-On Services</h3>
        <p class="text-gray-600 text-center mb-6">Make your move complete with these extras</p>
        <div class="grid grid-cols-2 md:grid-cols-3 lg:grid-cols-6 gap-4">
          {extraServices.map(service => (
            <div class="bg-gray-50 rounded-xl p-4 text-center hover:bg-[#FFA985]/10 transition-colors">
              <i class={`fas ${service.icon} text-[#FFA985] text-xl mb-2`}></i>
              <p class="font-medium text-[#333333] text-sm">{service.name}</p>
            </div>
          ))}
        </div>
      </div>
    </div>
  </section>

  <!-- Problems We Solve -->
  <section class="py-20 px-6 content-visibility-auto">
    <div class="container mx-auto">
      <div class="text-center mb-16">
        <h2 class="text-4xl md:text-5xl font-bold mb-4 text-[#333333]">
          Common Problems <span class="gold-gradient">We Solve</span>
        </h2>
        <div class="section-divider"></div>
        <p class="text-xl text-gray-600 max-w-3xl mx-auto">
          Moving is stressful enough - let us handle the cleaning challenges
        </p>
      </div>

      <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-8">
        {commonProblems.map(item => (
          <div class="bg-white rounded-2xl p-8 shadow-md border border-gray-200 hover:shadow-xl transition-shadow">
            <div class="w-16 h-16 rounded-full bg-gradient-to-br from-[#FFA985] to-[#FFC67D] flex items-center justify-center mb-4">
              <i class={`fas ${item.icon} text-white text-2xl`}></i>
            </div>
            <h3 class="text-xl font-bold mb-3 text-[#333333]">{item.problem}</h3>
            <p class="text-gray-600">{item.solution}</p>
          </div>
        ))}
      </div>
    </div>
  </section>

  <!-- Our Process -->
  <section class="py-20 px-6 bg-gray-50 content-visibility-auto">
    <div class="container mx-auto">
      <div class="text-center mb-16">
        <h2 class="text-4xl md:text-5xl font-bold mb-4 text-[#333333]">
          Our Move In/Out <span class="gold-gradient">Process</span>
        </h2>
        <div class="section-divider"></div>
        <p class="text-xl text-gray-600 max-w-3xl mx-auto">
          A systematic approach designed for empty homes and tight moving timelines
        </p>
      </div>

      <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-8">
        {processSteps.map(step => (
          <div class="relative bg-white rounded-2xl p-8 shadow-md border border-gray-200">
            <div class="absolute -top-4 -left-4 w-12 h-12 rounded-full bg-gradient-to-br from-[#FFA985] to-[#FFC67D] flex items-center justify-center text-white font-bold text-xl shadow-lg">
              {step.number}
            </div>
            <div class="ml-4">
              <div class="flex items-center mb-4">
                <i class={`fas ${step.icon} text-[#FFA985] text-2xl mr-3`}></i>
                <h3 class="text-xl font-bold text-[#333333]">{step.title}</h3>
              </div>
              <p class="text-gray-600">{step.description}</p>
            </div>
          </div>
        ))}
      </div>
    </div>
  </section>

  <!-- Why Choose Us -->
  <section class="py-20 px-6 content-visibility-auto">
    <div class="container mx-auto">
      <div class="text-center mb-16">
        <h2 class="text-4xl md:text-5xl font-bold mb-4 text-[#333333]">
          Why Choose <span class="gold-gradient">The Valley Clean Team</span>
        </h2>
        <div class="section-divider"></div>
        <p class="text-xl text-gray-600 max-w-3xl mx-auto">
          Trusted by renters, homeowners, landlords, and property managers
        </p>
      </div>

      <div class="grid md:grid-cols-2 lg:grid-cols-4 gap-8">
        <div class="text-center">
          <div class="w-20 h-20 mx-auto mb-4 rounded-full bg-gradient-to-br from-[#FFA985] to-[#FFC67D] flex items-center justify-center">
            <Icon name="medal" class="w-8 h-8 text-white" />
          </div>
          <h3 class="text-xl font-bold mb-3 text-[#333333]">Veteran Owned</h3>
          <p class="text-gray-600">Military precision, inspection-ready results.</p>
        </div>

